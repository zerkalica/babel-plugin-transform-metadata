[ignore]
dist/.*
build/.*
examples/.*
.*/node_modules/object.*
.*/node_modules/eslint.*
.*/node_modules/glob.*
.*/node_modules/bab.*
.*/node_modules/es5.*
.*/node_modules/esl.*
.*/node_modules/lodash.*
.*/node_modules/array.*
.*/node_modules/is-.*
.*/node_modules/object.*
.*/node_modules/acorn.*
.*/node_modules/fb.*
.*/node_modules/js.*
.*/node_modules/b.*
.*/node_modules/spdx.*
.*/node_modules/u.*
.*/node_modules/resol.*

[options]
esproposal.decorators=ignore
munge_underscores=true
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
module.name_mapper='^babel-plugin-transform-metadata/i/\(.*\)' -> '<PROJECT_ROOT>/i/\1'
module.name_mapper='^babel-plugin-transform-metadata/\(.*\)' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^babel-plugin-transform-metadata/_$' -> '<PROJECT_ROOT>/_'
module.name_mapper='^babel-plugin-transform-metadata/Deps$' -> '<PROJECT_ROOT>/Deps'
